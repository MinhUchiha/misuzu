.row-fruid
  .form-inline
    .form-group
      = label_tag :社員
      = select(:head, :shainbango, Shainmaster.all.collect {|p| [ p.氏名, p.社員番号 ] }, {selected: @shain_param, include_blank: true }, class: 'form-control')
    .form-group
      = label_tag :要件
      = select(:head, :youken, Kairanyokenmst.all.collect {|p| [ p.名称, p.id ] }, {selected: @yoken, include_blank: true }, class: 'form-control')
    -#.form-group
    -#  = label_tag :回答
    -#  = select(:head, :kaitou, Dengonkaitou.all.collect {|p| [ p.種類名, p.id ] }, {selected: @kaitou, include_blank: true }, class: 'form-control')
    .pull-right
      = button_tag '検索する', name: :button, id: :search, value: '検索する', class:'btn btn-primary'